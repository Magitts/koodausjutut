<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <directoryBrowse enabled="true" />
        <handlers accessPolicy="Read, Execute, Script">
            <add name="Python" path="*.py" verb="*" modules="CgiModule" scriptProcessor="&quot;C:\Program Files\Python36\python.exe&quot; s% s%" resourceType="File" requireAccess="Execute" />
        </handlers>
    </system.webServer>
</configuration>
